# Environment
NODE_ENV=development
PORT=3000

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/nexus_telephony
REDIS_URL=redis://localhost:6379

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRES_IN=7d

# API Configuration
API_VERSION=v1
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Twilio Compatibility
TWILIO_ACCOUNT_SID=your-account-sid
TWILIO_AUTH_TOKEN=your-auth-token

# African Carriers Configuration
# Nigeria
MTN_NG_API_KEY=your-mtn-nigeria-api-key
AIRTEL_NG_API_KEY=your-airtel-nigeria-api-key
GLO_NG_API_KEY=your-glo-nigeria-api-key
ETISALAT_NG_API_KEY=your-etisalat-nigeria-api-key

# Kenya
SAFARICOM_KE_API_KEY=your-safaricom-kenya-api-key
AIRTEL_KE_API_KEY=your-airtel-kenya-api-key

# Ghana
MTN_GH_API_KEY=your-mtn-ghana-api-key
AIRTELTIGO_GH_API_KEY=your-airteltigo-ghana-api-key
VODAFONE_GH_API_KEY=your-vodafone-ghana-api-key

# Uganda
MTN_UG_API_KEY=your-mtn-uganda-api-key
AIRTEL_UG_API_KEY=your-airtel-uganda-api-key

# Rwanda
MTN_RW_API_KEY=your-mtn-rwanda-api-key
AIRTEL_RW_API_KEY=your-airtel-rwanda-api-key

# Liberia
LONESTAR_LR_API_KEY=your-lonestar-liberia-api-key
ORANGE_LR_API_KEY=your-orange-liberia-api-key

# Payment Gateway
BITS_XPAY_API_KEY=your-bits-xpay-api-key
BITS_XPAY_SECRET=your-bits-xpay-secret
FLUTTERWAVE_PUBLIC_KEY=your-flutterwave-public-key
FLUTTERWAVE_SECRET_KEY=your-flutterwave-secret-key
PAYSTACK_PUBLIC_KEY=your-paystack-public-key
PAYSTACK_SECRET_KEY=your-paystack-secret-key

# AI/ML Services
OPENAI_API_KEY=your-openai-api-key
GOOGLE_CLOUD_API_KEY=your-google-cloud-api-key

# Monitoring and Logging
LOG_LEVEL=debug
SENTRY_DSN=your-sentry-dsn

# Regional Configuration
PRIMARY_REGION=africa-west
SECONDARY_REGIONS=africa-east,africa-south

# WebRTC Configuration
WEBRTC_STUN_SERVER=stun:stun.l.google.com:19302
WEBRTC_TURN_SERVER=turn:your-turn-server.com:3478
WEBRTC_TURN_USERNAME=your-turn-username
WEBRTC_TURN_CREDENTIAL=your-turn-credential